{
    "name": "lakion/sylius-api-php",
    "description": "Sylius API PHP client library",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5.0",
        "commerceguys/guzzle-oauth2-plugin": "~2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Michal Marcinkowski",
            "email": "michal.marcinkowski@lakion.com"
        }
    ],
    "autoload": {
        "psr-4": { "Sylius\\Api\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
